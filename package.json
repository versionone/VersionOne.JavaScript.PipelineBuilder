{
  "name": "@versionone/ctm-pipeline-builder-sdk",
  "version": "1.0.1",
  "description": "Create VersionOne Continuum pipeline definitions with javascript!",
  "main": "dist/index.js",
  "types": "dict/index.d.ts",
  "scripts": {
    "test": "npm run specs && npm run lint",
    "specs": "ava --verbose",
    "lint": "eslint -c .eslintrc --cache .",
    "build": "babel src -d dist"
  },
  "keywords": [
    "continuum",
    "versionone",
    "pipeline",
    "javascript"
  ],
  "author": "walkerrandolphsmith@gmail.com",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.10",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "eslint": "^2.5.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "stage-0"
    ],
    "plugins": [
      "espower",
      "transform-runtime"
    ],
    "ignore": [
      "*.spec.js"
    ]
  },
  "ava": {
    "files": [
      "*.spec.js"
    ],
    "source": [
      "./src/**/*.js"
    ],
    "concurrency": 10,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
